{
	"name": "electron-app",
	"version": "0.1.2",
	"description": "An Electron application with Vue and TypeScript",
	"main": "./out/main/index.js",
	"author": "example.com",
	"homepage": "https://www.electronjs.org",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "npm run typecheck:node && npm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "electron-vite dev --watch",
		"test": "electron-vite dev --watch --mode production",
		"icon": "eib -i ./resources/logo.png -o ./resources/icon",
		"build": "npm run typecheck && electron-vite build --mode production",
		"postinstall": "electron-builder install-app-deps",
		"build:win": "npm run build && electron-builder --win --config",
		"build:mac": "npm run build && electron-builder --mac --config",
		"build:linux": "npm run build && electron-builder --linux --config"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^2.0.0",
		"@electron-toolkit/utils": "^1.0.2",
		"@headlessui/vue": "^1.7.14",
		"axios": "^1.1.3",
		"cos-js-sdk-v5": "^1.4.15",
		"electron-store": "^8.1.0",
		"electron-updater": "^5.3.0",
		"js-cookie": "^3.0.1",
		"pinia": "^2.0.29",
		"qs": "^6.11.0",
		"toastify-js": "^1.11.2",
		"vue-tippy": "^6.0.0",
		"vue-router": "^4.1.6"
	},
	"devDependencies": {
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@electron/notarize": "^1.2.3",
		"@rushstack/eslint-patch": "^1.3.0",
		"@types/node": "^18.16.16",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"electron": "^24.4.1",
		"electron-builder": "^23.6.0",
		"electron-vite": "^1.0.23",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.14.1",
		"prettier": "^2.8.8",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-electron": "^0.11.1",
		"vite-plugin-electron-renderer": "^0.11.4",
		"vite-plugin-target": "^0.1.1",
		"vue": "^3.3.4",
		"vue-tsc": "^1.6.5"
	}
}
